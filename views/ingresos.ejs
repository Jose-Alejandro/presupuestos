<div class="row">
	<h2 class="text-center">INGRESOS</h2>
	<div id='Itable'>
		<div class="row">
			<div class="col-2">Concepto</div>
			<div class="col-2">Monto</div>
			<div class="col-1">Mes</div>
			<div class="col-2">Año</div>
			<div class="col-1"></div>
			<div class="col-1"></div>
		</div>
	</div>
</div>
<div class="row">
	<form id="newIngreso" class="row g-3 m-5">
		<div class="col-4">
			<label for="concept" class="form-label">Concepto</label>
			<input type="text" name="concept" class="form-control" placeholder="Concepto del ingreso">
		</div>
		<div class="col-2">
			<label for="amount" class="form-label">Monto</label>
			<input type="text" name="amount" class="form-control" placeholder="15000">
		</div>
		<div class="col-2">
			<label for="month" class="form-label">Mes</label>
			<input type="text" name="month" class="form-control" placeholder="1-12">
		</div>
		<div class="col-2">
			<label for="year" class="form-label">Año</label>
			<input type="text" name="year" class="form-control" placeholder="2021">
		</div>
		<div class="col-2">

		</div>
		<div class="col-12">
			<a id='addIngreso' class='btn btn-outline-success btn-lg'>Agregar Ingreso</a>
		</div>
	</form>
</div>
